# Search query.
PREFIX bd: <http://www.bigdata.com/rdf/search#>

SELECT (COUNT(*) as ?n) 

  WITH {
   SELECT *
    WHERE {
      ?lit bd:search "mike" .
      ?lit bd:relevance ?score .
      <http://www.bigdata.com/Mike> ?p ?lit .
      }
    ORDER BY DESC(?score)
    LIMIT 10
    OFFSET 0
  } as %searchSet1

WHERE {
   
   include %searchSet1
   
}
